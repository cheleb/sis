<rdf:RDF xml:lang     = "en"
         xmlns        = "http://usefulinc.com/ns/doap#"
         xmlns:rdf    = "http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:asfext = "http://projects.apache.org/ns/asfext#"
         xmlns:foaf   = "http://xmlns.com/foaf/0.1/">
  <Project rdf:about="http://sis.apache.org/">
    <created>2010-02-28</created>
    <name>Apache Spatial Information System</name>
    <shortdesc>A library for developing geospatial applications.</shortdesc>
    <programming-language>Java</programming-language>
    <homepage      rdf:resource = "http://sis.apache.org/"/>
    <category      rdf:resource = "http://projects.apache.org/category/library"/>
    <license       rdf:resource = "http://usefulinc.com/doap/licenses/asl20"/>
    <asfext:pmc    rdf:resource = "http://sis.apache.org"/>
    <bug-database  rdf:resource = "http://issues.apache.org/jira/browse/SIS"/>
    <mailing-list  rdf:resource = "http://sis.apache.org/mail-lists.html"/>
    <download-page rdf:resource = "http://sis.apache.org/downloads.html"/>
    <repository>
      <SVNRepository>
        <location rdf:resource="http://svn.apache.org/repos/asf/sis/trunk"/>
        <browse rdf:resource="http://svn.apache.org/viewvc/sis/trunk"/>
      </SVNRepository>
    </repository>
    <repository>
      <GitRepository>
        <location rdf:resource="git://git.apache.org/sis.git"/>
      </GitRepository>
    </repository>
    <asfext:implements>
      <asfext:Standard>
        <asfext:title>GeoAPI</asfext:title>
        <asfext:body>Open Geospatial Consortium</asfext:body>
        <asfext:id>OGC 09-083r3 </asfext:id>
        <asfext:url rdf:resource="http://www.opengeospatial.org/standards/geoapi"/>
      </asfext:Standard>
    </asfext:implements>
    <asfext:implements>
      <asfext:Standard>
        <asfext:title>Conceptual schema language</asfext:title>
        <asfext:body>International Organization for Standardization</asfext:body>
        <asfext:id>ISO 19103</asfext:id>
      </asfext:Standard>
    </asfext:implements>
    <asfext:implements>
      <asfext:Standard>
        <asfext:title>Metadata — Fundamentals</asfext:title>
        <asfext:body>International Organization for Standardization</asfext:body>
        <asfext:id>ISO 19115</asfext:id>
      </asfext:Standard>
    </asfext:implements>
    <asfext:implements>
      <asfext:Standard>
        <asfext:title>Metadata — Extensions for imagery and gridded data</asfext:title>
        <asfext:body>International Organization for Standardization</asfext:body>
        <asfext:id>ISO 19115-2</asfext:id>
      </asfext:Standard>
    </asfext:implements>
    <asfext:implements>
      <asfext:Standard>
        <asfext:title>Metadata — XML schema implementation</asfext:title>
        <asfext:body>International Organization for Standardization</asfext:body>
        <asfext:id>ISO 19139</asfext:id>
      </asfext:Standard>
    </asfext:implements>
    <asfext:implements>
      <asfext:Standard>
        <asfext:title>Spatial referencing by coordinates</asfext:title>
        <asfext:body>International Organization for Standardization</asfext:body>
        <asfext:id>ISO 19111</asfext:id>
      </asfext:Standard>
    </asfext:implements>
    <release>
      <Version>
        <name>SIS 0.4</name>
        <created>2014-04-06</created>
        <revision>0.4</revision>
      </Version>
    </release>
    <release>
      <Version>
        <name>SIS 0.3</name>
      </Version>
    </release>
    <release>
      <Version>
        <name>SIS 0.2-incubating</name>
      </Version>
    </release>
    <release>
      <Version>
        <name>SIS 0.1-incubating</name>
      </Version>
    </release>
  </Project>
</rdf:RDF>
